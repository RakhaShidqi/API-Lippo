<!DOCTYPE html>
<html>
<head>
  <title>Upload Success</title>
</head>
<body>
  <h2><%= message %></h2>

  <% if (preview && preview.length) { %>
    <h3>Preview mapped data (max 5 rows):</h3>
    <table border="1" cellpadding="5">
      <thead>
        <tr>
          <% Object.keys(preview[0]).forEach(col => { %>
            <th><%= col %></th>
          <% }) %>
        </tr>
      </thead>
      <tbody>
        <% preview.forEach(row => { %>
          <tr>
            <% Object.values(row).forEach(val => { %>
              <td><%= val %></td>
            <% }) %>
          </tr>
        <% }) %>
      </tbody>
    </table>
  <% } %>

  <a href="/lippo">Kembali ke Dashboard</a>
  <form action="/hypernet-lippo/insert-data" method="post">
    <button type="submit">Insert To Database</button>
  </form>
</body>
</html>
